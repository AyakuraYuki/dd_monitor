{
  "name": "dd_monitor",
  "version": "0.1.0",
  "description": "DD streams monitor, DD means 誰でも大好き",
  "main": "index.js",
  "scripts": {
    "setup": "flask init-schemas",
    "start": "electron .",
    "packager": "electron-packager ./ 'DD Monitor' --platform=all --arch=x64 --app-copyright=./LICENSE --app-version=0.1.0 --out=./package/ --overwrite --icon=./icon.ico --ignore=./ui --ignore=./node_modules/.bin --ignore=./.git --ignore=./.idea --ignore=./package",
    "builder": "rm -rf ./package; electron-builder -mwl --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AyakuraYuki/dd_monitor.git"
  },
  "author": "Ayakura Yuki",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/AyakuraYuki/dd_monitor/issues"
  },
  "homepage": "https://github.com/AyakuraYuki/dd_monitor#readme",
  "devDependencies": {
    "asar": "^2.0.1",
    "cross-env": "^5.2.1",
    "electron": "^6.0.8",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "productName": "DD Monitor",
    "appId": "cc.ayakurayuki.ddmonitor",
    "icon": "./icon.icns",
    "directories": {
      "output": "package"
    },
    "mac": {
      "target": [
        "zip"
      ]
    }
  }
}
