{
  "name": "dd_monitor",
  "version": "0.1.0",
  "description": "DD streams monitor, DD means 誰でも大好き",
  "main": "dist/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build; cp src/index.js dist/",
    "lint": "vue-cli-service lint",
    "clean": "rm -vrf ./package; rm -vrf ./dist",
    "start": "npm run build; export NODE_ENV=dev; electron .",
    "package": "npm run clean; npm run build; electron-builder --x64"
  },
  "author": "Ayakura Yuki",
  "license": "GNU General Public License v3.0",
  "homepage": "https://github.com/AyakuraYuki/dd_monitor",
  "bugs": {
    "url": "https://github.com/AyakuraYuki/dd_monitor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AyakuraYuki/dd_monitor.git"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.16.0",
    "download-git-repo": "^3.0.2",
    "electron-log": "^4.2.4",
    "minimist": "^1.2.5",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-pwa": "^4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/eslint-config-typescript": "^5.1.0",
    "asar": "^3.0.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^6.2.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vue-property-decorator": "^9.0.0",
    "vue-template-compiler": "^2.6.12"
  },
  "build": {
    "productName": "DD Monitor",
    "appId": "cc.ayakurayuki.ddmonitor",
    "copyright": "Copyright © 2019 Ayakura Yuki",
    "icon": "./icon.icns",
    "directories": {
      "buildResources": "build",
      "output": "package"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./package/.icon-ico/icon.ico",
      "uninstallerIcon": "./package/.icon-ico/icon.ico",
      "installerHeaderIcon": "./package/.icon-ico/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "DD Monitor"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "./icon.icns",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
